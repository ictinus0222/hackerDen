# Appwrite Configuration
# Copy this file to .env and replace with your actual values

# Your Appwrite server endpoint
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Your Appwrite project ID (found in your Appwrite console)
VITE_APPWRITE_PROJECT_ID=your-project-id-here

# Your Appwrite database ID (found in your Appwrite console)
VITE_APPWRITE_DATABASE_ID=your-database-id-here

# Your Appwrite API key (required for setup scripts - found in your Appwrite console under API Keys)
APPWRITE_API_KEY=your-api-key-here

# Optional: Additional configuration
# VITE_APP_NAME=HackerDen
# VITE_APP_VERSION=1.0.0