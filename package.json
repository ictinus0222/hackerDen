    "test:responsive": "vitest run src/components/__tests__/responsiveDesign.test.jsx",
    "test:snapshots": "vitest run src/components/__tests__/componentSnapshots.test.jsx",
    "test:visual-regression": "vitest run src/components/__tests__/visualRegression.test.jsx",
    "backup": "node scripts/backup-utility.js",
    "backup:restore": "node scripts/backup-utility.js --restore",
    "backup:cleanup": "node scripts/backup-utility.js --cleanup",
    "backup:cleanup-confirm": "node scripts/backup-utility.js --cleanup --confirm",
    "setup:whiteboard": "node scripts/setup-whiteboard.js",
    "setup:vault": "node scripts/setup-vault.js"
  },
